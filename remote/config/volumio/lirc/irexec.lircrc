begin
prog = irexec
button = KEY_ENTER
config = /usr/local/bin/volumio seek plus & 
end

begin
prog = irexec
button = KEY_UP
config = /usr/local/bin/volumio volume plus & 
delay = 2
repeat = 3
end

begin
prog = irexec
button = KEY_DOWN
config = /usr/local/bin/volumio volume minus & 
delay = 2
repeat = 3
end

begin
prog = irexec
button = KEY_RIGHT
config = /usr/local/bin/volumio next & 
end

begin
prog = irexec
button = KEY_LEFT
config = /usr/local/bin/volumio previous & 
end

begin
prog = irexec
button = KEY_MENU
config = /usr/local/bin/apessq2m toggle_input & 
end

begin
prog = irexec
button = KEY_PLAY
config = /usr/local/bin/volumio toggle & 
delay = 2
repeat = 3
end

